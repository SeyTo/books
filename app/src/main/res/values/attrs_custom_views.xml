<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string"/>
    <declare-styleable name="BookList">
        <attr name="title"/>
    </declare-styleable>
    <declare-styleable name="QuestionsView">
        <attr name="title"/>
        <attr name="question_title" format="string"/>
    </declare-styleable>
    <declare-styleable name="SellerList">
        <attr name="title"/>
    </declare-styleable>
    <declare-styleable name="MaskedEditTextLayout">
        <attr name="mask"/>
    </declare-styleable>
    <declare-styleable name="IconSwitch">
        <attr name="orientation" format="enum">
            <enum name="PORTRAIT" value="0"/>
            <enum name="LANDSCAPE" value="1"/>
        </attr>
        <attr name="baseColor" format="color"/>
        <attr name="actionOnDrawable" format="integer"/>
        <attr name="actionOffDrawable" format="integer"/>
    </declare-styleable>
</resources>
